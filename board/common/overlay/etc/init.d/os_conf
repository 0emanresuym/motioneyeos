
SYS_CONF="/etc/os.conf"
BOOT_CONF="/boot/os.conf"
DATA_CONF="/data/etc/os.conf"


# source in all conf files in order of precedence
if [[ -f ${SYS_CONF} ]]; then
    source ${SYS_CONF}
fi

if [[ -f ${DATA_CONF} ]]; then
    source ${DATA_CONF}
fi

if [[ -f ${BOOT_CONF} ]]; then
    source ${BOOT_CONF}
fi

unset SYS_CONF BOOT_CONF DATA_CONF

